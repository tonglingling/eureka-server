#!groovy
pipeline {
 agent any
 stages {
   stage('Build') {
     agent {
       docker {
         image 'maven:3-alpine'
         args '-v /root/.m2:/root/.m2'
       }
     }
     steps {
              agent any
              echo "start compile"
              sh "mvn -U -am clean install -Dmaven.test.skip=true "
     }
   }
 }
}